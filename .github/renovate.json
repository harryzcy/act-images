{
  "extends": [
    "github>harryzcy/renovate-config"
  ],
  "ignorePaths": [
    "requirements-*.txt"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/images/ubuntu/.*?/packages\\.json$/",
        "/images/ubuntu/.*?/README\\.md$/"
      ],
      "matchStrings": [
        "\"name\": \"Go\",\\n *?\"version\": \"(?<currentValue>.*?)\"",
        "Go (?<currentValue>\\d+\\.\\d+\\.\\d+)"
      ],
      "depNameTemplate": "go",
      "datasourceTemplate": "golang-version"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/images/ubuntu/.*?/packages\\.json$/",
        "/images/ubuntu/.*?/README\\.md$/"
      ],
      "matchStrings": [
        "\"name\": \"Node\",\\n *?\"version\": \"(?<currentValue>.*?)\"",
        "Node (?<currentValue>\\d+\\.\\d+\\.\\d+)"
      ],
      "depNameTemplate": "node",
      "datasourceTemplate": "node-version"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/images/ubuntu/.*?/packages\\.json$/",
        "/images/ubuntu/.*?/README\\.md$/"
      ],
      "matchStrings": [
        "\"name\": \"Python\",\\n *?\"version\": \"(?<currentValue>.*?)\"",
        "Python (?<currentValue>\\d+\\.\\d+\\.\\d+)"
      ],
      "depNameTemplate": "python",
      "datasourceTemplate": "python-version"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/images/ubuntu/.*?/packages\\.json$/",
        "/images/ubuntu/.*?/README\\.md$/"
      ],
      "matchStrings": [
        "\"name\": \"Rust\",\\n *?\"version\": \"(?<currentValue>.*?)\"",
        "Rust (?<currentValue>\\d+\\.\\d+\\.\\d+)",
        "\"name\": \"cargo\",\\n *?\"version\": \"(?<currentValue>.*?)\"",
        "cargo (?<currentValue>\\d+\\.\\d+\\.\\d+)"
      ],
      "depNameTemplate": "rust",
      "packageNameTemplate": "rust-lang/rust",
      "datasourceTemplate": "github-releases",
      "versioningTemplate": "semver"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/images/ubuntu/.*?/packages\\.json$/",
        "/images/ubuntu/.*?/README\\.md$/"
      ],
      "matchStrings": [
        "\"name\": \"npm\",\\n *?\"version\": \"(?<currentValue>.*?)\"",
        "npm (?<currentValue>\\d+\\.\\d+\\.\\d+)"
      ],
      "depNameTemplate": "npm",
      "datasourceTemplate": "npm"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/images/ubuntu/.*?/packages\\.json$/",
        "/images/ubuntu/.*?/README\\.md$/"
      ],
      "matchStrings": [
        "\"name\": \"pip\",\\n *?\"version\": \"(?<currentValue>.*?)\"",
        "pip (?<currentValue>\\d+\\.\\d+\\.\\d+)"
      ],
      "depNameTemplate": "pip",
      "datasourceTemplate": "pypi"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/images/ubuntu/.*?/packages\\.json$/",
        "/images/ubuntu/.*?/README\\.md$/"
      ],
      "matchStrings": [
        "\"name\": \"pipx\",\\n *?\"version\": \"(?<currentValue>.*?)\"",
        "pipx (?<currentValue>\\d+\\.\\d+\\.\\d+)"
      ],
      "depNameTemplate": "pipx",
      "datasourceTemplate": "pypi"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
